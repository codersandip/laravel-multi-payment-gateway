{
    "name": "yourname/laravel-multi-payment-gateway",
    "description": "A unified payment gateway wrapper for Indian-focused gateways (Razorpay, PayU, Stripe, Cashfree).",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your@email.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "VendorName\\MultiPayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VendorName\\MultiPayment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VendorName\\MultiPayment\\MultiPaymentServiceProvider"
            ],
            "aliases": {
                "MultiPayment": "VendorName\\MultiPayment\\Facades\\MultiPayment"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
